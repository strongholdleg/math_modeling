import numpy as np
a = [1, 2, 4]
b = np.array(a)
print(type(a))
print(type(b))
print(b * b)
print(b - b)
print(b / b)
A = np.zeros((2, 3))
print(A)
A[0, 2] = 5
print(A)
B = np.ones((3, 2))
print(B)
A = np.ndarray(shape=(2, 3))
print(A)
A = np.arange(0, 10, 1)
print(A)
B = np.arange(0, 5, 0.5)
print(B)
print(A[-1])
A = np.array([[1,2,3],[4,5,6]])
print(A[1,:])
B = A[:, ::-1]
print('A',A)
